{
    "name": "auth-service",
    "version": "1.0.0",
    "description": "The user management service",
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "dev": "cross-env NODE_ENV=dev nodemon src/server.ts",
        "format:fix": "prettier . --write",
        "format:check": "prettier . --check",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
